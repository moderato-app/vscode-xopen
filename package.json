{
  "name": "xopen",
  "displayName": "Xopen",
  "description": "Open current line in Xcode",
  "version": "1.0.5",
  "publisher": "moderato-app",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/moderato-app/vscode-xopen"
  },
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.93.1"
  },
  "categories": [
    "Other",
    "Programming Languages"
  ],
  "keywords": [
    "Open in Xcode",
    "xcode",
    "open",
    "jump",
    "xed",
    "apple",
    "goto",
    "go to"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "xopen.open-in-xcode",
        "title": "Open in Xcode"
      }
    ],
    "keybindings": [
      {
        "command": "xopen.open-in-xcode",
        "key": "alt alt"
      }
    ]
  },
  "menus": {
    "editor/context": [
      {
        "command": "xopen.open-in-xcode",
        "group": "navigation"
      }
    ],
    "explorer/context": [
      {
        "command": "xopen.open-in-xcode",
        "group": "navigation"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "ci": "pnpm run compile && pnpm run lint && pnpm eslint src && pnpm vsce package --no-dependencies",
    "package": "pnpm vsce package --no-dependencies",
    "publish": "pnpm vsce publish --no-dependencies --target darwin-x64 --target darwin-arm64"
  },
  "packageManager": "pnpm@9.6.0",
  "devDependencies": {
    "@types/vscode": "^1.93.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^3.3.2"
  }
}